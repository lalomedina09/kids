{
    "name": "queriodinero/base",
    "type": "project",
    "description": "The Querido Dinero platform.",
    "keywords": [
        "queridodinero"
    ],
    "license": "MIT",
    "repositories": {
        "queridodinero/advice": {
            "type": "vcs",
            "url": "git@bitbucket.org:queridodinero/qd-advice.git"
        },
        "queridodinero/communications": {
            "type": "vcs",
            "url": "git@bitbucket.org:queridodinero/qd-comm.git"
        },
        "queridodinero/marketplace": {
            "type": "vcs",
            "url": "git@bitbucket.org:queridodinero/qd-market.git"
        },
        "queridodinero/products": {
            "type": "vcs",
            "url": "git@bitbucket.org:queridodinero/qd-products.git"
        }
    },
    "require": {
        "php": "^7.1.3",
        "aws/aws-sdk-php": "3.81.*",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "bugsnag/bugsnag-laravel": "2.15.*",
        "conekta/conekta-php": "^4.0",
        "doctrine/dbal": "^2.9",
        "facebook/php-business-sdk": "*",
        "fideloper/proxy": "^4.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "6.3",
        "hashids/hashids": "^3.0",
        "jenssegers/date": "3.5.*",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.8.*",
        "laravel/helpers": "^1.1",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.8.*",
        "lcobucci/jwt": "3.3.3",
        "league/csv": "^9.5",
        "league/flysystem-aws-s3-v3": "1.0.*",
        "meetdemio/demio-php-sdk": "dev-master",
        "mews/purifier": "^3.3",
        "misd/linkify": "^1.1",
        "paypal/rest-api-sdk-php": "^1.14",
        "predis/predis": "1.1.*",
        "pusher/pusher-php-server": "^4.0",
        "queridodinero/advice": "1.5.25",
        "queridodinero/communications": "^0.1",
        "queridodinero/marketplace": "0.5.*",
        "queridodinero/products": "^0.1",
        "spatie/laravel-analytics": "3.6.*",
        "spatie/laravel-medialibrary": "7.5.*",
        "spatie/laravel-newsletter": "^4.2",
        "spatie/laravel-permission": "^2.33",
        "twilio/sdk": "^6.18"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/browser-kit-testing": "4.0.*",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
